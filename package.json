{
	"name": "preact-typescript-webpack4-boilerplate",
	"version": "1.0.0",
	"description": "Preact training app",
	"scripts": {
		"build": "npm run clean && webpack --config ./webpack.config.js --mode production --progress",
		"dev": "npm run clean && webpack --config ./webpack.config.js --mode development",
		"start": "webpack-dev-server --config ./webpack.config.js --mode development",
		"clean": "rimraf ./dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lexey111/preact-typescript-webpack4-boilerplate.git"
	},
	"keywords": [
		"preact",
		"typescript",
		"less",
		"boilerplate",
		"webpack 4"
	],
	"alias": {
		"preact": "./node_modules/preact/dist/preact.min.js"
	},
	"author": "lexey111 <lexey111@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"isomorphic-unfetch": "^3.0.0",
		"preact": "^8.2.9",
		"query-string": "^6.2.0"
	},
	"devDependencies": {
		"@types/facebook-js-sdk": "^3.2.0",
		"@types/query-string": "^6.2.0",
		"copy-webpack-plugin": "^4.5.2",
		"css-loader": "^2.1.0",
		"file-loader": "^1.1.11",
		"fork-ts-checker-webpack-plugin": "^0.4.3",
		"less": "^3.8.0",
		"less-loader": "^4.1.0",
		"less-plugin-clean-css": "^1.5.1",
		"mini-css-extract-plugin": "^0.5.0",
		"node-sass": "^4.11.0",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"ts-loader": "^4.4.2",
		"typescript": "^2.9.2",
		"webpack": "^4.16.3",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.5"
	}
}
